GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MBProgressHUD" "${PODS_ROOT}/Headers/Public/MPColorTools" "${PODS_ROOT}/Headers/Public/Tml"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/Public/MPColorTools" -isystem "${PODS_ROOT}/Headers/Public/Tml"
OTHER_LDFLAGS = -ObjC -l"Pods-Flipagram-MBProgressHUD" -l"Pods-Flipagram-MPColorTools" -l"Pods-Flipagram-Tml" -framework "CoreGraphics"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods